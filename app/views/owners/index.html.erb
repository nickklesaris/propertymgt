<h1> Owners </h1>
<table>
  <thead>
    <tr>
      <th>First Name </th>
      <th>Last Name </th>
      <th>email </th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
    <tr>
      <td><%= owner.first_name %></td>
      <td><%= owner.last_name %> </td>
      <td><%= owner.email %> </td>
    </tr>
    <br>
  </tbody>
</table>
<%- end -%>
